{
  "version": 2,
  "description": "Nuranest Pregnancy AI API - Vercel Deployment Configuration",
  "builds": [
    {
      "src": "app/main.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "app/main.py"
    }
  ],
  "env": {
    "PYTHONPATH": "."
  }
} 